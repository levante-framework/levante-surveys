{
  "name": "levante-surveys",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-deploy": "npm run build-only && vercel --prod",
    "deploy:prod:alias": "node scripts/vercel-deploy-alias.js",
    "vercel-build": "npm run build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "translations:guard:no-es": "node scripts/check-no-es.js",
    "sync:en-gh": "node scripts/sync-en-gh.js --all",
    "smoke:child": "node scripts/smoke-test-child.js",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "download-crowdin:api": "node scripts/download-crowdin-files.js --folder /Surveys-with-en_US",
    "download-crowdin-bundle": "crowdin bundle download 20",
    "download-crowdin-bundle:split": "node scripts/download-crowdin-bundle.js --force --backup",
    "update-surveys": "npm run download-crowdin-bundle && node scripts/split-crowdin-bundle.js --force && node scripts/import-individual-surveys.js --all",
    "update-surveys:deploy": "npm run download-crowdin-bundle && node scripts/split-crowdin-bundle.js --force && node scripts/import-individual-surveys.js --all --upload",
    "convert-language-codes": "node scripts/convert-language-codes.js",
    "convert-language-codes:all": "node scripts/convert-language-codes.js --all",
    "process-surveyjs-update": "npm run convert-language-codes:all && npm run import-surveys-individual:all",
    "process-surveyjs-update:deploy": "npm run convert-language-codes:all && npm run import-surveys-individual:upload",
    "extract-surveys": "node scripts/extract-translations.js",
    "extract-surveys:child": "node scripts/extract-translations.js surveys/child_survey.json",
    "extract-surveys:all": "npm run extract-surveys surveys/child_survey.json && npm run extract-surveys surveys/parent_survey_family.json && npm run extract-surveys surveys/parent_survey_child.json && npm run extract-surveys surveys/teacher_survey_general.json && npm run extract-surveys surveys/teacher_survey_classroom.json",
    "xliff:export:json": "node scripts/export-xliff-from-json.js",
    "xliff:export:all": "node scripts/export-xliff-from-json.js --all",
    "xliff:import": "node scripts/import-xliff-into-json.js",
    "xliff:import:combined": "node scripts/import-xliff-combined.js",
    "xliff:import:all": "node scripts/import-xliff-all.js",
    "xliff:download:all": "node scripts/download-crowdin-xliff.js",
    "xliff:update:all": "npm run xliff:download:all && npm run xliff:import:all",
    "xliff:validate": "node scripts/validate-updated-surveys.js",
    "pipeline:dev": "npm run xliff:download:all -- --force && npm run xliff:import:all && npm run xliff:validate && npm run deploy-surveys:dev",
    "normalize:defaults:one": "node scripts/normalize-survey-defaults.js",
    "normalize:defaults:all": "node scripts/normalize-survey-defaults.js --all",
    "xliff:apply:child": "node scripts/xliff-apply-all.js surveys/child_survey.json",
    "xliff:apply:parent-family": "node scripts/xliff-apply-all.js surveys/parent_survey_family.json",
    "xliff:apply:parent-child": "node scripts/xliff-apply-all.js surveys/parent_survey_child.json",
    "xliff:apply:teacher-general": "node scripts/xliff-apply-all.js surveys/teacher_survey_general.json",
    "xliff:apply:teacher-classroom": "node scripts/xliff-apply-all.js surveys/teacher_survey_classroom.json",
    "xliff:upload:dev": "node scripts/upload-to-dev-xliff.js",
    "xliff:crowdin:upload:sources": "node scripts/xliff-crowdin-cli.js upload-sources",
    "xliff:crowdin:upload:translations": "crowdin upload translations --config crowdin-xliff.yml",
    "xliff:crowdin:download": "crowdin download --config crowdin-xliff.yml",
    "import-surveys": "node scripts/import-translations.js",
    "import-surveys:child": "node scripts/import-translations.js surveys/child_survey.json",
    "import-surveys:all": "npm run import-surveys surveys/child_survey.json && npm run import-surveys surveys/parent_survey_family.json && npm run import-surveys surveys/parent_survey_child.json && npm run import-surveys surveys/teacher_survey_general.json && npm run import-surveys surveys/teacher_survey_classroom.json",
    "import-surveys-combined": "node scripts/import-combined-translations.js",
    "import-surveys-combined:legacy": "node scripts/import-combined-translations.js surveys/surveys.csv",
    "import-surveys-combined:upload": "node scripts/import-combined-translations.js surveys/surveys.csv --upload",
    "generate-pdfs": "node scripts/generate-survey-pdfs.js",
    "generate-pdfs:german": "node scripts/generate-survey-pdfs-german.js",
    "reset-translations": "node scripts/reset-en-gh-translations.js",
    "copy-es-co-to-es": "node scripts/copy-es-co-to-es.cjs",
    "copy-es-co-to-es:upload": "node scripts/copy-es-co-to-es.cjs --upload",
    "translations:remove-es": "node scripts/remove-es-translations.js",
    "detect-languages": "node scripts/detect-languages.js",
    "detect-languages:all": "node scripts/detect-languages.js --analyze-all",
    "detect-languages:surveys": "node scripts/detect-languages.js surveys/surveys.csv",
    "deploy-surveys": "node scripts/deploy-surveys.js",
    "deploy-surveys:dev": "node scripts/deploy-updated-dev.js",
    "deploy-surveys:prod": "node scripts/deploy-surveys.js --env=PROD",
    "deploy-surveys:dry-run": "node scripts/deploy-surveys.js --dry-run",
    "deploy-surveys:force": "node scripts/deploy-surveys.js --env=DEV --force",
    "deploy-surveys:skip-download": "node scripts/deploy-surveys.js --env=DEV --skip-download",
    "upload-crowdin": "crowdin upload sources",
    "upload-crowdin:surveys-folder": "node scripts/upload-sources-batch.js --folder /Surveys-with-en_US",
    "upload-crowdin:surveys-folder:dry": "node scripts/upload-sources-batch.js --folder /Surveys-with-en_US --dryrun",
    "crowdin:copy-en-to-en-gh": "node scripts/crowdin-copy-en-to-en-gh.js",
    "crowdin:copy-en-to-en-gh:dry": "node scripts/crowdin-copy-en-to-en-gh.js --dry-run",
    "generate:en-us": "node scripts/generate-en-us-translations.js",
    "upload:en-us": "crowdin upload translations -l en-US -s surveys/*_translations.csv -t /surveys/*_translations_%locale%.csv",
    "merge:crowdin": "node scripts/merge-crowdin-into-local.js",
    "merge:crowdin:seed-en-us": "node scripts/merge-crowdin-into-local.js --seed-en-us",
    "add:en-us-column": "node scripts/add-en-us-column.js",
    "upload-crowdin:check": "crowdin status",
    "extract-and-upload": "npm run extract-surveys:all && npm run upload-crowdin",
    "setup-test-surveys": "node scripts/setup-test-surveys.js",
    "test:e2e": "npm run setup-test-surveys && cypress run",
    "test:e2e:open": "npm run setup-test-surveys && cypress open",
    "test:surveys": "npm run setup-test-surveys && cypress run --spec 'cypress/e2e/surveys.cy.js'",
    "test:surveys:open": "npm run setup-test-surveys && cypress open --e2e --spec 'cypress/e2e/surveys.cy.js'",
    "test:locales": "npm run setup-test-surveys && cypress run --spec 'cypress/e2e/survey-locales.cy.js'",
    "test:locales:open": "npm run setup-test-surveys && cypress open --e2e --spec 'cypress/e2e/survey-locales.cy.js'",
    "test:content": "npm run setup-test-surveys && cypress run --spec 'cypress/e2e/survey-content-validation.cy.js'",
    "test:content:open": "npm run setup-test-surveys && cypress open --e2e --spec 'cypress/e2e/survey-content-validation.cy.js'",
    "test:all": "npm run setup-test-surveys && cypress run --spec 'cypress/e2e/*.cy.js'",
    "local:sync:updated": "node scripts/sync-updated-to-public.js",
    "generate:surveyjs": "node scripts/generate-standard-terms.js",
    "xliff:export:surveyjs": "node scripts/export-xliff-from-json.js fixes/surveyjs.json --source-lang en-US",
    "crowdin:upload:surveyjs": "node scripts/upload-xliff-source.js xliff-out/surveyjs/surveyjs-source.xliff --dest /main/surveys/surveyjs.xliff",
    "crowdin:upload:surveyjs:translations": "node scripts/upload-xliff-translations.js",
    "crowdin:download:surveyjs": "node scripts/download-surveyjs-translations.js",
    "crowdin:fetch:surveyjs": "node scripts/crowdin-fetch-surveyjs.js",
    "apply:surveyjs:terms": "node scripts/apply-surveyjs-terms.js",
    "crowdin:download:surveyjs": "node scripts/download-surveyjs-translations.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@primevue/core": "^4.3.6",
    "@primevue/themes": "^4.3.6",
    "axios": "^1.5.1",
    "lodash": "^4.17.21",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.0.0",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.6",
    "puppeteer": "^24.16.1",
    "survey-core": "^2.3.7",
    "survey-creator-core": "^2.3.7",
    "survey-creator-vue": "^2.3.7",
    "survey-vue3-ui": "^2.3.7",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/lodash": "^4.14.196",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^13.17.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "inputmask": "^5.0.9",
    "jiti": "^2.4.2",
    "nouislider": "^15.8.1",
    "npm-run-all2": "^8.0.4",
    "signature_pad": "^5.1.1",
    "surveyjs-widgets": "^1.12.54",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}
