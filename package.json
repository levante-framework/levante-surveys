{
  "name": "levante-surveys",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "download-surveys": "node scripts/download-surveys.js",
    "extract-translations": "node scripts/extract-translations.js",
    "extract-translations:child": "node scripts/extract-translations.js surveys/child_survey.json",
    "extract-translations:all": "npm run extract-translations surveys/child_survey.json && npm run extract-translations surveys/parent_survey_family.json && npm run extract-translations surveys/parent_survey_child.json && npm run extract-translations surveys/teacher_survey_general.json && npm run extract-translations surveys/teacher_survey_classroom.json",
    "import-translations": "node scripts/import-translations.js",
    "import-translations:child": "node scripts/import-translations.js surveys/child_survey.json",
    "import-translations:all": "npm run import-translations surveys/child_survey.json && npm run import-translations surveys/parent_survey_family.json && npm run import-translations surveys/parent_survey_child.json && npm run import-translations surveys/teacher_survey_general.json && npm run import-translations surveys/teacher_survey_classroom.json"
  },
  "dependencies": {
    "@primevue/core": "^4.3.6",
    "@primevue/themes": "^4.3.6",
    "axios": "^1.5.1",
    "lodash": "^4.17.21",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.6",
    "survey-core": "^2.0.2",
    "survey-vue3-ui": "^2.0.2",
    "survey-creator-core": "^2.0.2",
    "survey-creator-vue": "^2.0.2",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/lodash": "^4.14.196",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}
